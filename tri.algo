PROCEDURE tri_insertion(tab[1:n])
VAR
    n: INTEGER;
    i,j:INTEGER;
    elm= tab[i];

BEGIN
    FOR i FROM 0 TO tab.length-1 STEP 1 DO
    i=j
    elm = tab[i]
            WHILE (j>0 && tab[j-1] > elm) DO
                tab[j]=tab[j-1]
                j--
            END_WHILE
            tab[j] ) elm
    END_FOR
END